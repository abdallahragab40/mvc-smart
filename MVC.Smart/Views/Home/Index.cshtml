
@{
    ViewBag.Title = "Home";
}

<h1> Welcome to my Application </h1>
<button onclick="startAjax()" class="btn btn-success">Ajax</button>
@Html.ActionLink("download", "TestActionResult","Home")
@Html.Action("TestActionResult")

@section Scripts {
    <script>
        function startAjax() {
            $.ajax({
                url: "/home/TestActionResult",
                type: "Get",
                success : function(res, status, xhr) {
                    console.log(res);
                },
                error: function (xhr, status, err) {
                    console.log(err);
                }
            })
        }
    </script>
}

